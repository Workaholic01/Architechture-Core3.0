@model IEnumerable<LVT.Models.Entities.VehicleAlarm>


<div class="content-header">
    <div class="d-flex align-items-center">
        <div class="mr-auto">
            <h3 class="page-title">All Alarms</h3>
        </div>
    </div>
</div>


<section class="content">

    <div class="box">
        <div class="box-body">
            <form role="form">
                <div class="form-group">
                    <div class="row">
                        <div class="col-lg-2">
                            <label>Alarm Type</label>
                            <select class="form-control">
                                <option>Facial recognition</option>
                                <option>Live Vehicle Tracking</option>
                                <option>option 3</option>
                                <option>option 4</option>
                                <option>option 5</option>
                            </select>
                        </div>
                        <div class="col-lg-2">
                            <label>Source</label>
                            <input type="text" class="form-control" placeholder="Enter Source">
                        </div>
                        <div class="col-lg-2">
                            <label>Generation Time (From)</label>
                            <input type="date" class="form-control">
                        </div>
                        <div class="col-lg-2">
                            <label>Generation Time (To)</label>
                            <input type="date" class="form-control">
                        </div>
                        <div class="col-lg-2">
                            <label>Status</label>
                            <select class="form-control">
                                <option>All</option>
                                <option>option 2</option>
                                <option>option 3</option>
                                <option>option 4</option>
                                <option>option 5</option>
                            </select>
                        </div>
                        <div class="col-lg-2" style="margin-top: 28px;">
                            <button type="button" class="btn btn-primary btn-sm mb-5"><i class="fa fa-search"></i>&nbsp;Search</button>
                            <button type="button" class="btn btn-warning btn-sm mb-5"><i class="fa fa-undo"></i>&nbsp;Reset</button>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-1 text-center" style="margin-top: 8px;">
            <div class="checkbox checkbox-success">
                <input id="checkbox1" type="checkbox">
                <label for="checkbox1"> Select All</label>
            </div>
        </div>
        <div class=col-lg-2>
            <a class="btn"><i class="fa fa-refresh"></i>&nbsp;Refresh</a>
            <a class="btn"><i class="fa fa-download"></i>&nbsp;Export</a>
        </div>
    </div>
    <div class="row  mt-4">
        <div class="col-12 box-deck">
            <div class="box">
                <img class="box-img-top" src="../images/person-zoom.png" alt="box image cap" data-toggle="modal" data-target=".bs-example-modal-lg" class="model_img img-fluid">
                <div class="box-body">
                    <ul class="list-unstyled">
                        <li><span class="text-muted">Alarm Type :</span> <span class="text-warning">Facial recognition blacklist target.</span></li>
                        <li><span class="text-muted">Alarm Content :</span> <span class="">558 Qazi Town Bus Stop..</span></li>
                        <li><span class="text-muted">Generation Time :</span> <span class="">2019-05-11 10:55:21</span></li>
                        <li><span class="text-muted">Alarm Algorithm :</span> <span class="">yitu_face_global_sdk</span></li>
                        <li><span class="text-muted">Status :</span> <span class="text-danger">Unprocessed</span></li>
                    </ul>
                    <div class="row mt-4">
                        <div class="col-lg-6">
                            <div class="checkbox checkbox-success">
                                <input id="checkbox1" type="checkbox">
                                <label for="checkbox1"></label>
                            </div>
                        </div>
                        <div class="col-lg-6 text-right">
                            <a class="text-white"><i class="fa fa-edit"></i></a>
                        </div>
                    </div>
                </div>
            </div>
            @foreach (var item in Model)
            {

                <div class="box">
                    <img class="box-img-top model_img img-fluid" src="@item.ImagePath" alt="box image cap" data-toggle="modal" data-target=".bs-example-modal-lg0" onclick="ShowModal('@item.ImagePath','@item.AlarmLocation','@item.AlarmGenerationTime','@item.Status')">
                    <div class="box-body">
                        <ul class="list-unstyled">
                            <li><span class="text-muted">Alarm Type :</span> <span class="text-warning">Live Vehicle Tracking</span></li>
                            <li><span class="text-muted">Alarm Content :</span> <span class="">@item.AlarmLocation</span></li>
                            <li><span class="text-muted">Generation Time :</span> <span class="">@item.AlarmGenerationTime</span></li>
                            <li><span class="text-muted">Alarm Algorithm :</span> <span class="">@item.AlarmLocation</span></li>
                            <li><span class="text-muted">Status :</span> <span class="text-danger">@item.Status</span></li>
                        </ul>
                        <div class="row mt-4">
                            <div class="col-lg-6">
                                <div class="checkbox checkbox-success">
                                    <input id="checkbox1" type="checkbox">
                                    <label for="checkbox1"></label>
                                </div>
                            </div>
                            <div class="col-lg-6 text-right">
                                <a class="text-white"><i class="fa fa-edit"></i></a>
                            </div>
                        </div>
                    </div>
                </div>
            }
