                        </div>
                    </div>
                </div>*@
            <div class="box">
                <img class="box-img-top" src="../images/vehicle3-zoom.png" alt="box image cap" data-toggle="modal" data-target=".bs-example-modal-lg3" class="model_img img-fluid">
                <div class="box-body">
                    <ul class="list-unstyled">
                        <li><span class="text-muted">Alarm Type :</span> <span class="text-warning">Live Vehicle Tracking</span></li>
                        <li><span class="text-muted">Alarm Content :</span> <span class="">125-4-Regal Chowk P2</span></li>
                        <li><span class="text-muted">Generation Time :</span> <span class="">2019-10-01 12:05:21</span></li>
                        <li><span class="text-muted">Alarm Algorithm :</span> <span class="">yitu_face_global_sdk</span></li>
