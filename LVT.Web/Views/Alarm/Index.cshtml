                    </ul>
                    <div class="row mt-4">
                        <div class="col-lg-6">
                            <div class="checkbox checkbox-success">
                                <input id="checkbox1" type="checkbox">
                                <label for="checkbox1"></label>
                            </div>
                        </div>
                        <div class="col-lg-6 text-right">
                            <a class="text-white"><i class="fa fa-edit"></i></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade bs-example-modal-lg0" tabindex="-1" role="dialog" id aria-labelledby="myLargeModalLabel" aria-hidden="true" style="display: none;">
        <div class="modal-dialog modal-lg vehicle-modal">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title" id="myLargeModalLabel">Alarms</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-lg-12">
                            <img id="ImgUrl" width="100%">
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-6">
                            <ul class="list-unstyled mt-4">
                                <li><span class="text-muted">Alarm Type :</span> <span id="alarmType" class="text-warning"></span></li>
                                <li><span class="text-muted">Alarm Content :</span> <span id="alarmContent" class=""></span></li>
                                <li><span class="text-muted">Generation Time :</span> <span id="GenerationTime" class=""></span></li>
                            </ul>
                        </div>
                        <div class="col-lg-6">
                            <ul class="list-unstyled mt-4">
                                <li><span class="text-muted">Alarm Algorithm :</span> <span id="alarmAlgorithm" class=""></span></li>
                                <li><span class="text-muted">Status :</span> <span id="alarmStatus" class="text-danger"></span></li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="modal-footer text-right" style="padding-top: 13px !important;">
                    <a href="/mdt" type="button" class="btn btn-secondary waves-effect text-left btn-sm">Process to MDT</a>
                    <button type="button" class="btn btn-secondary waves-effect text-left btn-sm">Process to PUCAR</button>
                    <button type="button" class="btn btn-secondary waves-effect text-left btn-sm">Discard</button>
                    <button type="button" class="btn btn-secondary waves-effect text-left btn-sm" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-secondary waves-effect text-left btn-sm" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
</section>


<script>
    function ShowModal(ImgUrl, alarmContent, GenerationTime, alarmStatus) {

        $('#ImgUrl').attr('src', ImgUrl);
        $('#alarmContent').text(alarmContent);
        $('#GenerationTime').text(GenerationTime);
        $('#alarmAlgorithm').text(alarmAlgorithm);
        $('#alarmStatus').text(alarmStatus);
        // $('#myModal').modal('show');
    }
</script>